{
    "version": 2,
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.py"
        },
        {
            "src": "/health",
            "dest": "/api/index.py"
        },
        {
            "src": "/docs",
            "dest": "/api/index.py"
        },
        {
            "src": "/openapi.json",
            "dest": "/api/index.py"
        },
        {
            "src": "/",
            "dest": "/frontend/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/$1"
        }
    ]
}